#!/bin/bash
######################################################################
# ugcancel 0.1
#-------------------------------------------------------------------
# created by Martin Rupp 05.03.2012
######################################################################

scriptpath=`dirname $0`

echo "uginfo 0.1. (c) Goethe-Center for Scientific Computing 2012"

function usage 
{
	echo ""
    exit 0;
}

while [ $# -gt 0 ]
do
	if [ $1 = "-cluster" ]; then
		UGSUBMIT_TYPE=$2
		shift 2
	else
		break
	fi
done


source $scriptpath/clusters

UJS_Info

